@using TestApplication.Models
@model User

@{
    ViewBag.Title = "All users";
}

<h2>All users</h2>
<label>@Html.ActionLink("Add new user", "Edit", "User", new { id = -1 }, null)</label>

@foreach (User user in ViewBag.Users)
{
    <label style="display: inline">
        @user.FirstName &nbsp; 
        @user.LastName &nbsp;  
        @Html.ActionLink("Edit", "Edit", "User", new { id = user.Id }, null)
        @Html.ActionLink("Delete", "Delete", "User", new { id = user.Id }, null)
    </label>
    <br />
}